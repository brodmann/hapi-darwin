{
    "extends": "hapi",
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "hapi/hapi-capitalize-modules": false,
        "arrow-parens": ["error", "as-needed", { "requireForBlockBody": true }]
    }
}
